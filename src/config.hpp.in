/* Define this macro if Schnek is compiled with MPI support */
#cmakedefine SCHNEK_HAVE_MPI

/* Define this macro if Schnek is compiled with HDF5 support */
#cmakedefine SCHNEK_HAVE_HDF5

/* Defined if we use parallel file access for HDF5 (mpio) if available. */
#cmakedefine SCHNEK_USE_HDF_PARALLEL

/* define if the Kokkos library is available */
#cmakedefine SCHNEK_HAVE_KOKKOS

/* Integer Schnek Version Number */
#cmakedefine SCHNEK_VERSION @SCHNEK_VERSION@

/* Skip C++ definitions when using Open MPI */
#define OMPI_SKIP_MPICXX